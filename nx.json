{
  "npmScope": "muse.js",
  "affected": { "defaultBase": "master" },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "nativescript": { "tags": [] },
    "nativescript-e2e": {
      "tags": [],
      "implicitDependencies": ["nativescript"]
    },
    "nest-sls": { "tags": [] },
    "nest-sls-e2e": { "tags": [], "implicitDependencies": ["nest-sls"] }
  },
  "workspaceLayout": { "appsDir": "e2e", "libsDir": "packages" }
}
