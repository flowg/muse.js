          }
        }
      }
    },
    "nest-sls-e2e": {
      "projectType": "application",
      "root": "e2e/nest-sls-e2e",
      "sourceRoot": "e2e/nest-sls-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
